@model List<CategoryDto>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Category Name</th>
            <th scope="col">Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (CategoryDto item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>
                    <a class="btn btn-secondary" href="/Category/Edit/@item.Id">Edit</a>
                    <a class="btn btn-danger" href="/Category/Delete/@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>